{
    "manifestVersion": 1,
    "id": "GitCrossCommit",
    "version": "1.1.1",
    "name": "Git-Cross-Commit",
    "description": "VSTS Custom Task for committing a build artifact to a 2nd Git repository.",
    "publisher": "Cyber-Ninjas",
    "galleryFlags": [
        "Private"
    ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "icons": {
        "default": "images/icon.png"
    },
    "categories": [
        "Azure Pipelines"
    ],
    "tags": [
        "build",
        "git"
    ],
    "content": {
        "details": {
            "path": "README.md"
        },
        "license": {
            "path": "LICENSE"
        }
    },
    "contributions": [
        {
            "id": "f8f54430-c86b-11e9-888a-97daa5f7b82e",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "GitCrossCommit"
            }
        }
    ],
    "files": [
        {
            "path": "GitCrossCommit"
        },
        {
            "path": "images",
            "addressable": true
        }
    ],
    "links": {
        "learn": {
            "uri": "https://github.com/CyberNinjas/VSTS-Git-Cross-Commit/blob/master/README.md"
        },
        "repository": {
            "uri": "https://github.com/CyberNinjas/VSTS-Git-Cross-Commit.git"
        },
        "license": {
            "uri": "https://github.com/CyberNinjas/VSTS-Git-Cross-Commit/blob/master/LICENSE"
        },
        "issues": {
            "uri": "https://github.com/CyberNinjas/VSTS-Git-Cross-Commit/issues"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/CyberNinjas/VSTS-Git-Cross-Commit.git"
    }
}
